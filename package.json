{
    "name": "turborepo-test",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/frontend/*",
        "apps/backend/*",
        "configs/*",
        "layout/*",
        "services/*",
        "types/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --no-cache --parallel --continue",
