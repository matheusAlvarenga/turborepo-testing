        "test:e2e": "turbo run test:e2e",
        "storybook": "turbo run storybook"
    },
    "devDependencies": {
        "eslint": "^8.18.0",
        "turbo": "latest"
    },
    "engines": {
        "npm": ">=8.0.0",
        "node": ">=16.0.0"
    },
    "dependencies": {},
    "packageManager": "yarn@1.22.17"
}
